{
	"name": "my-vercel-project",
	"alias": "my-vercel-project",
	"builds": [
		{
			"src": "frontend",
			"use": "@vercel/static-build"
		},
		{
			"src": "api/*",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/api/$1"
		},
		{
			"src": "/(.*)",
			"dest": "/frontend/$1"
		}
	]
}
